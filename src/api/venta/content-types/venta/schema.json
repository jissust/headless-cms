{
  "kind": "collectionType",
  "collectionName": "ventas",
  "info": {
    "singularName": "venta",
    "pluralName": "ventas",
    "displayName": "venta"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre_venta": {
      "type": "string",
      "customField": "plugin::my-custom-fields.input-nombre-venta"
    },
    "nombre_local": {
      "type": "string",
      "customField": "plugin::my-custom-fields.input-nombre-local"
    },
    "descripcion": {
      "type": "text"
    },
    "tipo_de_venta": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tipo-de-venta.tipo-de-venta"
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "telefono": {
      "type": "string"
    },
    "dni": {
      "type": "biginteger"
    },
    "email": {
      "type": "email"
    },
    "local": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::local.local"
    },
    "Productos": {
      "type": "dynamiczone",
      "components": [
        "productos.productos"
      ]
    },
    "total": {
      "type": "decimal",
      "customField": "plugin::my-custom-fields.my-input-number-total-venta-field",
      "default": 0
    },
    "total_ganancia": {
      "type": "decimal",
      "customField": "plugin::my-custom-fields.input-total-venta-ganancia",
      "default": 0
    },
    "tipo_de_moneda": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tipo-de-moneda.tipo-de-moneda"
    },
    "forma_de_pago": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::forma-de-pago.forma-de-pago"
    },
    "fecha_de_ingreso": {
      "type": "date",
      "required": true
    },
    "sales_detect_changes_in_items": {  
      "type": "string",
      "customField": "plugin::my-custom-fields.sales-detect-changes-in-items"
    }
  }
}
