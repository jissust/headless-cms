{
  "kind": "collectionType",
  "collectionName": "gastos",
  "info": {
    "singularName": "gasto",
    "pluralName": "gastos",
    "displayName": "gasto"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "proveedor": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "telefono": {
      "type": "biginteger"
    },
    "local": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::local.local"
    },
    "tipo_de_moneda": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tipo-de-moneda.tipo-de-moneda"
    },
    "fecha_de_ingreso": {
      "type": "date",
      "required": true
    },
    "Gastos": {
      "type": "dynamiczone",
      "components": [
        "gastos.gastos-items"
      ]
    },
    "total": {
      "type": "decimal",
      "customField": "plugin::my-custom-fields.input-total-gastos"
    }
  }
}
